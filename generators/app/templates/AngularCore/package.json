{
  "name": "<%= project %>",
  "version": "1.0.0",
  "private": false,
  "keywords": [
    "aspnetcore",
    "entityframework core",
    "angular2",
    "webpack2",
    "typescript2",
    "material design"
  ],
  "author": "<%= author %>",
  "scripts": {
    "rimraf": "rimraf",
    "tslint": "tslint",
    "webpack": "webpack",
    "protractor": "protractor",
    "clean:aot": "npm run rimraf -- compiled",
    "clean:dist": "npm run rimraf -- wwwroot/dist",
    "clean:start": "npm start",
    "build:prod": "node node_modules/webpack/bin/webpack.js --config config/webpack.prod.js --env.prod && npm run precache",
    "build:dev": "set ASPNETCORE_ENVIRONMENT=Development && node node_modules/webpack/bin/webpack.js --config config/webpack.dev.js --env.dev && npm run precache",
    "build:dotnet": "dotnet restore",
    "precache": "sw-precache --verbose --config=Config/sw-precache-config.js",
    "dev:watch": "set ASPNETCORE_ENVIRONMENT=Development && dotnet watch run",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "lint": "npm run tslint \"Client/**/*.ts\"",
    "postinstall": "npm run webdriver:update",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e": "npm run protractor",
    "e2e:live": "npm-run-all -p -r server:prod:ci e2e:debug",
    "e2e:debug": "protractor --elementExplorer",
    "test": "npm run lint && karma start",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "webdriver-manager update"
  },
  "dependencies": {
    "@angular/animations": "4.2.6",
